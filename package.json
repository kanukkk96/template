{
  "name": "ee-static-build-template",
  "version": "0.0.0",
  "description": "",
  "main": "",
  "etherealEngine": {
    "version": "1.3.0"
  },
  "scripts": {
    "test": "mocha --config .mocharc.js",
    "dev": "cross-env APP_ENV=development NODE_OPTIONS=--max_old_space_size=20480 vite",
