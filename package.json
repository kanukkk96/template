    "precommit": "no-master-commits -b master"
  },
  "peerDependencies": {},
  "dependencies": {},
  "devDependencies": {},
  "license": "ISC",
  "pre-commit": [
    "format-staged"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  }
